<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.project.springbootcurrency.mapper.ChinicMapper">
    <select id="getChinic" resultType="cn.project.springbootcurrency.pojo.Chinic">
        select * from chinic
    </select>
    <update id="UpdateChinic">
        update chinic set name=#{name},owner=#{owner},phone=#{phone},province=#{province},city=#{city},address=#{address}
        ,introduce=#{introduce},state=#{state},expire_time=#{expire_time},email=#{email} where id = #{id}
    </update>
</mapper>